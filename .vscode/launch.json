    "version": "0.2.0",
    "configurations": [
        {
            "name": "run :: webserver",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "src/restaurant_app"
            },
            "args": [
                "run"
            ],
            "jinja": true,
            "autoStartBrowser": false,
            "envFile": "${workspaceFolder}/src/.env"
        },
        {
          "name": "run :: import",
          "type": "debugpy",
          "request": "launch",
          "module": "flask",
          "env": {
              "FLASK_APP": "src/restaurant_app"
          },
          "args": [
              "db",
              "import",
              "./data/initial_restaurant_data.json"
          ],
          "jinja": true,
          "autoStartBrowser": false,
          "envFile": "${workspaceFolder}/src/.env"
      }
    ]
}
